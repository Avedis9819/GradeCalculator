        android:ems="10"
        android:inputType="number"
        android:text="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.470
" />

    <Button
        android:id="@+id/calculateButton"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:layout_marginTop="472dp"
        android:layout_marginEnd="92dp"
        android:text="Calculate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/resetHomeworks"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:layout_marginTop="520dp"
        android:layout_marginEnd="92dp"
        android:text="Reset Homeworks"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/participationGradeInput"
        android:layout_width="223dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:ems="10"
        android:inputType="number"
        android:text="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.170" />

    <EditText
        android:id="@+id/midterm1GradeInput"
        android:layout_width="223dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:ems="10"
        android:hint="Midterm1"
        android:inputType="number"
        android:text="10"
        app:layout_constraintBottom_toBottomOf="parent"
